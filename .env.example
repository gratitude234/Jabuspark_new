SUPABASE_URL=https://your-supabase-project.supabase.co
SUPABASE_KEY=public-anon-key
SUPABASE_SERVICE_KEY=service-role-key

# keep Nuxt runtime vars in sync with Supabase keys above when needed
NUXT_SUPABASE_URL=${SUPABASE_URL}
NUXT_SUPABASE_ANON_KEY=${SUPABASE_KEY}
NUXT_SUPABASE_SERVICE_KEY=${SUPABASE_SERVICE_KEY}

# Gemini configuration
NUXT_GEMINI_API_KEY=your-gemini-api-key
NUXT_GEMINI_MODEL_TEXT=models/gemini-2.5-flash
NUXT_GEMINI_MODEL_EMBEDDING=models/text-embedding-004
NUXT_GEMINI_DISABLED=false # set to true to disable Gemini-backed features

